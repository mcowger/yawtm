{
  "name": "yawtm",
  "version": "1.2.1",
  "description": "A custom git worktree manager",
  "type": "module",
  "main": "./dist/tm.js",
  "bin": {
    "tm": "tm.ts"
  },
  "files": [
    "tm.ts",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "bun run tm.ts",
    "build": "bun build tm.ts --outdir ./dist --target bun",
    "dev": "bun run tm.ts",
    "compile": "bun build --compile --target=bun-darwin-arm64 tm.ts --outfile ./dist/tm-darwin-arm64 && bun build --compile --target=bun-linux-x64 tm.ts --outfile ./dist/tm-linux-x64",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "git",
    "worktree",
    "manager",
    "cli"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "bun-types": "latest"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "dependencies": {
    "@octokit/rest": "^22.0.1",
    "simple-git": "^3.30.0"
  }
}
